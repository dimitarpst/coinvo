generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Expense {
  id        String   @id @default(uuid())
  amount    Decimal
  currency  String
  category  String
  date      DateTime
  note      String?
  createdAt DateTime @default(now())
}